<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nano_Windows_Timer"
        xmlns:Timers="clr-namespace:System.Timers;assembly=System.ComponentModel.TypeConverter" x:Name="MainTimerWindow" x:Class="Nano_Windows_Timer.MainWindow"
        mc:Ignorable="d"
        Title="Timer" Height="450" Width="500" Foreground="{x:Null}" FontSize="16" RenderTransformOrigin="0.5,0.5" Background="#FF282828">
    <Window.LayoutTransform>
        <TransformGroup>
            <ScaleTransform/>
            <SkewTransform/>
            <RotateTransform/>
            <TranslateTransform X="0" Y="0"/>
        </TransformGroup>
    </Window.LayoutTransform>
    <Window.Effect>
        <DropShadowEffect/>
    </Window.Effect>
    <Window.RenderTransform>
        <TransformGroup>
            <ScaleTransform/>
            <SkewTransform/>
            <RotateTransform Angle="0"/>
            <TranslateTransform/>
        </TransformGroup>
    </Window.RenderTransform>

    <Grid>
    </Grid>
</Window>
